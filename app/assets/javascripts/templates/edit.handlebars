<div class="col-xs-10 col-sm-10 col-md-10 col-lg-10"></div>
<div class="float-clear"></div>

<br>
<br>
<div class=" col-xs-8 col-sm-8 col-md-8  col-lg-8 col-xs-offset-2 col-sm-offset-2  col-md-offset-2 col-lg-offset-2">
 <div class="panel panel-primary">
  <div class="panel-heading">
       <h4 class="">Edit {{profile_name}}</h4>
  </div>

  <div class="panel-body">
  

  <div id="userOptions" class="tab-pane active fade in col-xs-12 col-sm-12 col-md-12  col-lg-12">
   <form {{action edit  on="submit"}} class="col-xs-12 col-sm-12 col-md-12  col-lg-12 form-horizontal"> 
      <div class="errors-one"></div>
   
      <br>

          <div class="form-group"><label class="control-label col-xs-3 col-sm-3 col-md-3  col-lg-3"  >Current Password</label> 
           <div class="col-xs-7 col-sm-7 col-md-7  col-lg-7">{{view Ember.TextField value=current_password id="current_password" type="password" class="form-control" autofocus="true" placeholder="required" required="true"}}</div><div class="col-xs-2 col-sm-2 col-md-2  col-lg-2"></div></div>

          <div class="form-group"><label class="control-label col-xs-3 col-sm-3 col-md-3  col-lg-3"  >New Password</label> 
           <div class="col-xs-7 col-sm-7 col-md-7  col-lg-7">{{view Ember.TextField value=password id="password" type="password" class="form-control" pattern=".{8,}" title="Passwords must be at least 8 characters."}}</div><div class="col-xs-2 col-sm-2 col-md-2  col-lg-2"></div></div>

          <div class="form-group"><label class="control-label col-xs-3 col-sm-3 col-md-3  col-lg-3"  >Confirm Password</label> 
           <div class="col-xs-7 col-sm-7 col-md-7  col-lg-7">{{view Ember.TextField value=password_confirmation id="password_confirmation" type="password" class="form-control" pattern=".{8,}" title="Passwords must be at least 8 characters."}}</div><div class="col-xs-2 col-sm-2 col-md-2  col-lg-2"></div></div>



      <div class="form-group "><label class="control-label col-xs-3 col-sm-3 col-md-3  col-lg-3"> First Name</label>
       <div class="col-xs-7 col-sm-7 col-md-7  col-lg-7">{{view Ember.TextField value=first_name id="first_name" type="text" class="form-control" }}</div>
          <div class="checkbox col-xs-2 col-sm-2 col-md-2  col-lg-2">
          <label>
            {{view Ember.Checkbox checked=first_name_privacy}} Public
          </label>
        </div>
       </div>

       <div class="form-group "><label class="control-label col-xs-3 col-sm-3 col-md-3  col-lg-3"> Last Name</label>
       <div class="col-xs-7 col-sm-7 col-md-7  col-lg-7">{{view Ember.TextField value=last_name id="last_name" type="text" class="form-control" }}</div>
          <div class="checkbox col-xs-2 col-sm-2 col-md-2  col-lg-2">
          <label>
            {{view Ember.Checkbox checked=last_name_privacy}} Public
          </label>
        </div>
       </div>



          <div class="form-group "><label class="control-label col-xs-3 col-sm-3 col-md-3  col-lg-3"> Email</label>
       <div class="col-xs-7 col-sm-7 col-md-7  col-lg-7">{{view Ember.TextField value=email id="email" type="email" class="form-control" }}</div>
          <div class="checkbox col-xs-2 col-sm-2 col-md-2  col-lg-2">
          <label>
            {{view Ember.Checkbox checked=email_privacy}} Public
          </label>
        </div>
       </div>

  



          <div class="form-group "><label class="control-label col-xs-3 col-sm-3 col-md-3  col-lg-3"> Location</label>
       <div class="col-xs-7 col-sm-7 col-md-7  col-lg-7">{{view Ember.TextField value=location id="location" type="text" class="form-control" }}</div>
          <div class="checkbox col-xs-2 col-sm-2 col-md-2  col-lg-2">
          <label>
            {{view Ember.Checkbox checked=location_privacy}} Public
          </label>
        </div>
       </div>

          <div class="form-group "><label class="control-label col-xs-3 col-sm-3 col-md-3  col-lg-3"> Rating</label>
       <div class="col-xs-7 col-sm-7 col-md-7  col-lg-7">{{view Ember.Select content=possibleRatings id="rating" value=rating class="form-control"}}</div>
          <div class="checkbox col-xs-2 col-sm-2 col-md-2  col-lg-2">
          <label>
            {{view Ember.Checkbox checked=rating_privacy}} Public
          </label>
        </div>
       </div>

          <div class="form-group "><label class="control-label col-xs-3 col-sm-3 col-md-3  col-lg-3"> Title</label>
       <div class="col-xs-7 col-sm-7 col-md-7  col-lg-7">{{view Ember.Select  content=possibleTitles id="title" value=title class="form-control" }}</div>
          <div class="checkbox col-xs-2 col-sm-2 col-md-2  col-lg-2">
          <label>
            {{view Ember.Checkbox checked=title_privacy}} Public
          </label>
        </div>
       </div>

            <div class="form-group"><label class="control-label col-xs-3 col-sm-3 col-md-3  col-lg-3">Avatar</label> 
           <div class="col-xs-7 col-sm-7 col-md-7  col-lg-7" ><p class="form-control-static text-center"> <a href="https://public-api.wordpress.com/oauth2/authorize?client_id=1854&response_type=code&blog_id=0&state=5e7a5c0a6d609e0b5dd8502ccb5560c9386d8f71ec95bf63f04b64ca1ce71f5d&redirect_uri=https%3A%2F%2Fen.gravatar.com%2Fconnect%2F%3Faction%3Drequest_access_token"><b>Create/Change Avatar with Gravatar!</b></a></p></div><div class="col-xs-2 col-sm-2 col-md-2  col-lg-2"></div></div>

          <div class="form-group"><label class="control-label col-xs-3 col-sm-3 col-md-3  col-lg-3" > Other</label>
           <div class="col-xs-7 col-sm-7 col-md-7  col-lg-7">{{view Ember.TextArea value=other id="other" class="form-control"}}</div> 
            <div class="checkbox col-xs-2 col-sm-2 col-md-2  col-lg-2">
              <label>
                {{view Ember.Checkbox checked=other_privacy}} Public
              </label>
            </div></div>


          
          <div class="text-center form-group col-xs-12 col-sm-12 col-md-12  col-lg-12"><button  type="submit" class="btn btn-primary" value="Login">Edit</button></div>
        
      </form>
        </div>
      </div>
    </div>
</div>
     <div class="col-xs-2 col-sm-2  col-md-2 col-lg-2">
  </div>
